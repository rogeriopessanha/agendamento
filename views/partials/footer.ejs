<% if (link=='cadastro' ) { %>

    <script src="/jquery.mask.js"></script>
    <script>
        $('.cpf').mask('000.000.000-00');
    </script>

    <% } %>
        <% if (link=='inicio' ) { %>
            <script>
                var element = document.querySelector('#calendar');
                var calendar = new FullCalendar.Calendar(element, {
                    initialView: 'dayGridMonth',
                    locale: 'pt-br',
                    events: 'calendario',
                    height: 'auto',
                    editable: true,
                    selectable: true,
                    selectMirror: true,
                    nowIndicator: true,
                    eventClick: function (info) {
                        window.location.href = `/event/${info.event.id}`;
                    },
                    headerToolbar: {
                        left: 'prev,next',
                        center: 'title',
                        right: ''
                    },
                });

                calendar.render();
            </script>
            <% } %>
