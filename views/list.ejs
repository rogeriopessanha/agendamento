
<h3>Consultas</h3>
<hr>
<form action="/searchresult" method="get">
    <input type="search" name="search" id="search" placeholder="Pelo CPF ou Email">
    <button>Pesquisar</button>
</form>
<hr>
<% appos.forEach(appo => { %>
    <h3>Informações</h3>
    <h4>Paciente: <%= appo.name %></h4>
    <h4>Email: <%= appo.email %></h4>
    <h4>CPF: <%= appo.cpf %></h4>
    <h4>Assunto: <%= appo.description %></h4>
    <hr>
<% }) %>

